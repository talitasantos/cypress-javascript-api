{
    "name": "cypress-javascript-api",
    "version": "1.0.0",
    "description": "Projeto de teste de API usando Cypress",
    "main": "index.js",
    "scripts": {
        "test": "cypress run",
        "test:createBoards": "cypress run --spec 'cypress/integration/api/createBoards.spec.js'",
        "test:createCards": "cypress run --spec 'cypress/integration/api/createCards.spec.js'",
        "test:deleteBoards": "cypress run --spec 'cypress/integration/api/deleteBoards.spec.js'",
        "test:deleteCards": "cypress run --spec 'cypress/integration/api/deleteCards.spec.js'",
        "cypress:open": "cypress open"
    },
    "keywords": [
        "cypress",
        "api-testing",
        "test-automation"
    ],
    "author": "Talita Ribeiro",
    "license": "ISC",
    "devDependencies": {
        "@bahmutov/cy-api": "^1.4.3",
        "cypress": "^12.17.4",
        "cypress-plugin-api": "^2.0.1",
        "mochawesome-merge": "^4.3.0",
        "mochawesome-report-generator": "^6.2.0",
        "typescript": "^5.6.3"
    },
    "dependencies": {
        "ajv": "^8.17.1",
        "dotenv": "^16.4.5",
        "node-fetch": "^2.7.0"
    }
}
